<div class="span12">
  <div class="page-header">
    <h1>
      <a href="/" class="logo"><%= name_of_site %></a><br>
    </h1>
  </div>

  <div class="span7 cloudsSummary">
    <a href="/" class="homeNav">Home</a>
    
    <h3>Word Clouds by Decade!</h3>
    
    <p>Each cloud is based on the phrases that were most popular in <i>New York Times</i> wedding announcements from that particular decade relative to the other decades</p>
  </div>
  
  <div class="clearfix"></div>
  
  <div class="cloudSocialLinks">
    <span id="tweetButton"></span>
    <!-- <div class="fb-like" data-href="http://www.weddingcrunchers.com" data-width="90" data-layout="button_count" data-show-faces="false" data-send="false"></div> -->
  </div>
  
  <% Article.wordcloud_urls.each do |url| %>
    <div class="wordcloud">
      <%= link_to(image_tag(url), url, :target => "_blank") %>
    </div>
  <% end %>
</div>